  "scripts": {
    "build": "next build",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn dev-server\" \"next\" \"ts-node -P tsconfig.local.json dev.ts\"",
    "dev-server": "ts-node-dev -r dotenv/config --respawn --ignore-watch node_modules --ignore-watch .next -P tsconfig.local.json --inspect -- src/server/api.ts",
    "server-test": "ts-node-dev  -r dotenv/config --respawn --ignore-watch node_modules --ignore-watch .next -P tsconfig.local.json src/server/api.ts",
    "start": "cross-env NODE_ENV=production \"yarn start-server\" \"next start\"",
    "start-server": "ts-node -r dotenv/config -P tsconfig.local.json -T src/server/api.ts",
    "test": "cross-env NODE_ENV=test PORT=9999 jest --detectOpenHandles --forceExit",
    "tsc": "tsc"
  },
