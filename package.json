{
  "name": "cotizafacil",
  "version": "1.0.0",
  "private": false,
  "repository": "https://github.com/PabloSzx/CotizaFacil.git",
  "license": "MIT",
  "author": "PabloSz <pablosaez1995@gmail.com>",
  "scripts": {
    "api-inspect": "kill-port 27050 && node --inspect=27050 ./api/dist/server.js",
    "api-test": "run-s dev-api",
    "build": "run-p -l build-*",
    "build-api": "rm -rf ./api/dist && tsc -P tsconfig.local.json",
    "build-client": "next build",
    "dev": "cross-env NODE_ENV=development run-p -l dev-*",
    "dev-api": "rm -rf ./api/dist && tsc-watch -P tsconfig.local.json --onSuccess \"yarn api-inspect\"",
    "dev-client": "wait-on tcp:4000 && next",
    "dev-proxy": "wait-on -l tcp:3000 tcp:4000 && ts-node -P tsconfig.local.json dev.ts",
    "start": "cross-env NODE_ENV=production run-p start-*",
    "start-api": "node ./api/dist/server.js",
    "start-client": "next start -p 6001",
    "test": "cross-env NODE_ENV=test PORT=9999 jest --detectOpenHandles --forceExit",
    "tsc": "tsc --noEmit"
  },
  "resolutions": {
    "class-validator": "0.11.0"
  },
